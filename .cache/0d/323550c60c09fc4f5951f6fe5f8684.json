;\n  justify-content: center;\n  font-size: calc(10px + 2vmin);\n  color: white;\n}\n\n.App-link {\n  color: #61dafb;\n}\n\n@keyframes App-logo-spin {\n  from {\n    transform: rotate(0deg);\n  }\n  to {\n    transform: rotate(360deg);\n  }\n}\n\n\n\n\n\n.battleCard {\n\n  width: 600px;\n  min-height: 100px;\n  background-color: #f6f8f9;\n  float: left;\n  position: relative;\n  padding: 15px;\n  padding-top: 8px;\n  box-sizing: border-box;\n  margin-bottom: 30px;\n  margin-top: 20px;\n  margin-left: 20px;\n  border-style: ridge;\n  border-width: '10px 10px 10px 10px'; \n    \n\n  /* overflow-x: visible;\n  overflow-y: scroll; */\n/*   background-color:rgb(228, 222, 222);  \n\n  max-height: 700px;\n  width: 500px;\n  overflow: auto;\n  border-style: ridge;\n  border-width: '10px 10px 10px 10px';\n  \n\n  min-height: 100px;\n  float: left;\n  position: relative;\n  padding: 15px;\n  padding-top: 8px;\n  padding-right: 10px;\n  box-sizing: border-box;\n  margin-bottom: 30px; */ \n\n\n}\n\n.cards {\n  width: calc(100% - 30px);\n  background-color: white;\n  position: relative;\n\n  float: left;\n  min-height: 80px;\n  padding: 15px;\n  padding-top: 8px;\n  padding-right: inherit;\n  display: inline-block;\n  margin-bottom: 20px;\n\n  box-sizing: border-box;\n\n  \n  border-style: ridge;\n  border-width: '10px 10px 10px 10px';  \n  \n\n\n}\n\n.deleteCardButton {\n  background-color: rgb(230, 111, 111);\n  float: right; \n}\n\n.addCardButton {\n  /* background-color: rgb(93, 151, 240);\n  float: center; */\n  position: relative;\n  display: inline-block;\n  cursor: pointer;\n  width: 35px;\n  height: 35px; \n  background-color: var(--secondary);\n  color: white;\n  border-radius: 100%;\n  text-align: center;\n  box-sizing: border-box;\n  padding-top: 1px;\n  text-align: center;\n}\n\n.leftMenu {\n  width: 190px;\n  min-height: 400px;\n  background: white;\n  float: left;\n  margin-top: 5px;\n  margin-right: 10px;\n  padding: 0px 10px;\n  overflow-x: hidden;\n  white-space: nowrap;\n  transition: 0.5s;\n  border-style: ridge;\n  font-size: 10px;\n  text-align: left;\n  margin-top: 20px;\n  font-size: 14px;\n\n}\n\n.titleBattleCard {\n  font-size: 18px;\n  color: var(--secondary);\n  border: none;\n  outline: none;\n  font-weight: 500;\n  margin: 0px;\n  padding: 0px;\n  width: 380px;\n  resize: none;\n}\n\n.smallIndex {\n  width: 100%;\n  overflow: auto;\n  border-style: ridge;\n  background-color: white;\n}\n\n.unselectedSmallIndexButton {\n  float: left;\n  font-size: 14px;\n  color: #444;\n  padding: 7.5px 15px;\n  cursor: pointer;\n  padding: 5.5px 13px;\n  margin: 4px;\n  border-radius: 5px;\n  transition: 0.1s;\n}\n\n.selectedSmallIndexButton {\n  background: #444;\n  float: left;\n  font-size: 14px;\n  color: white;\n  padding: 7.5px 15px;\n  cursor: pointer;\n  padding: 5.5px 13px;\n  margin: 4px;\n  border-radius: 5px;\n  transition: 0.1s;\n}\n\n.addBattleCard {\n  width: 60px;\n  margin: 20px 20px 0px 10px;\n  height: 100%;\n  float: left;\n}\n\n.leftMenuItems {\n  color: rgb(80, 80, 80);\n  font-size: 16px;\n  margin: 5px 0px;\n  cursor: pointer;\n}\n\n.closeAllOpenedBattleCardsButton {\n  color: white;\n  /* background: var(--secondary); */\n  background: rgb(62, 113, 207);\n  text-align: center;\n  padding: 4px 0px;\n  font-size: 12px;\n  border-radius: 20px;\n  margin: 30px 0px;\n  cursor: pointer;\n  user-select: none;\n}\n.addCardMenu {\n  width: 70%;\n  margin-left: 15%;\n  margin-right: 15%;\n  background-color: white;\n  padding: 10px;\n  display: inline-block;\n  box-sizing: border-box;\n  border-style:ridge;\n}\n\n.unfilledAddButton {\n  width: 45%;\n  margin: 2.5%;\n  box-sizing: border-box;\n  display: inline-block;\n  float: left;\n  background-color: ghostwhite;\n  height: 40px;\n  color: var(--secondary);\n  padding-top: 7px;\n  cursor: pointer;\n  font-size: 15px;\n}\n\n.redAddButton {\n  width: 45%;\n  margin: 2.5%;\n  box-sizing: border-box;\n  display: inline-block;\n  float: left;\n  background-color: ghostwhite;\n  height: 40px;\n  color: var(--red);\n  padding-top: 7px;\n  cursor: pointer;\n  font-size: 15px;\n}\n\n.battleCardNameHeader {\n  font-size: 16px;\n  font-weight: 900;\n  margin-left: 5px;\n  float: right;\n  padding: 0px 3px;\n  color: var(--secondary);\n  text-align: right;\n  line-height: 27px;\n}\n\n.blueCardHeader {\n  color: var(--secondary);\n  font-size: 16px;\n  font-weight: 900;\n}\n\n.redCardHeader {\n  color: var(--red);\n  font-size: 16px;\n  font-weight: 900;\n}\n\n.blueCardText {\n  color: var(--secondary);\n  font-size: 15px;\n  width: 100%;\n  margin-top: 10px;\n  resize: none;\n  outline: none;\n  background: transparent;\n  border: none;\n  overflow: hidden;\n}\n\n.redCardText {\n  color: var(--red);\n  font-size: 15px;\n  width: 100%;\n  margin-top: 10px;\n  resize: none;\n  outline: none;\n  background: transparent;\n  border: none;\n}\n\n.blueHeaderValues {\n  font-size: 12px;\n  outline: none;\n  border: none;\n  float: right;\n}"},"lineCount":321}},"error":null,"hash":"06c6065d7ef27d8c6d63797775d58edb","cacheData":{"env":{}}}